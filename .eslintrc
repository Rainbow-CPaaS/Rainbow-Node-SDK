{
    "ecmaFeatures": {},
    "parser": "espree",
    "env": {
        "browser": true,
        "amd": true,
        "phantomjs": true,
        "jquery": true,
        "node": false,
        "mocha": false,
        "jasmine": false,
        "qunit": true
    },
    "globals": {
        "requirejs": true,
        "MediaStreamTrack": true,
        "chrome": true,
        "Promise": true,
        "reject": true,
        "angular": true,
        "Strophe": true,
        "$iq": true,
        "$pres": true,
        "$msg": true,
        "config": true,
        "process": true,
        "app": true,
        "emojione": true,
        "adapter": true,
        "Base64": true,
        "Ba64": true,
        "RBError": true,
        "VCard": true,
        "version": true,
        "FileTransfert": true,
        "moment": true,
        "MD5": true,
        "toByteArray": true,
        "Uint8Array": true,
        "Console.Re": true,
        "base64EncArr": true,
        "base64DecToArr": true,
        "anonymizePhoneNumber": true,
        "Clipboard": true,
        /* MOCHA */
        "describe": false,
        "it": false,
        "before": false,
        "beforeEach": false,
        "after": false,
        "afterEach": false,
        "inject": false,
        "sinon": false,
        "expect": false,
        "assert": false,
        "DetectRTC": true,
        "sdk": true,
        "Nanobar": true
    },
    "plugins": ["node"],
    "extends": ["eslint:recommended", "plugin:node/recommended"],
    "rules": {
        /** Possible errors **/
        "comma-dangle": [
            0,
            "always-multiline"
        ], // to be fixed
        "no-cond-assign": 2,
        "no-console": 0,
        "no-constant-condition": 2,
        "no-control-regex": 2,
        "no-debugger": 2,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty-character-class": 2,
        "no-empty": 2,
        "no-ex-assign": 2,
        "no-extra-boolean-cast": 2,
        "no-extra-parens": [
            2,
            "functions"
        ],
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-inner-declarations": [
            2,
            "functions"
        ],
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-negated-in-lhs": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-sparse-arrays": 2,
        "no-unexpected-multiline": 2,
        "no-unreachable": 2,
        "use-isnan": 2,
        "valid-jsdoc": 0, // to be fixed
        "valid-typeof": 2,
        /** Legacy **/
        "max-depth": [
            2,
            5
        ],
        "no-bitwise": 0,
        /** Variables **/
        "no-catch-shadow": 2,
        "no-delete-var": 2,
        "no-label-var": 2,
        "no-shadow-restricted-names": 2,
        "no-shadow": 2,
        "no-undef-init": 2,
        "no-undef": 2,
        "no-undefined": 0,
        "no-unused-vars": [
            2,
            {
                "vars": "all",
                "args": "all",
                "argsIgnorePattern": "^__" // ignore all unused function arguments with 2 leading underscore
            }
        ],
        /* Strict mode */
        "strict": 1,
        /* Best practices */
        "accessor-pairs": 2,
        "complexity": [
            2,
            30
        ], // to be fixed
        "consistent-return": 1,
        "curly": [
            2,
            "all"
        ],
        "default-case": 2,
        "dot-location": [
            2,
            "property"
        ],
        "dot-notation": [
            2,
            {
                "allowKeywords": true
            }
        ],
        "eqeqeq": 2,
        "guard-for-in": 1,
        "no-alert": 2,
        "no-caller": 2,
        //"no-case-declarations": 2,
        "no-div-regex": 2,
        "no-else-return": 2,
        "no-labels": 2,
        "no-empty-pattern": 2,
        "no-eq-null": 2,
        "no-eval": 2,
        "no-extend-native": [
            2,
            {
                "exceptions": [
                    "String"
                ]
            }
        ],
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implicit-coercion": 2,
        "no-implied-eval": 2,
        "no-invalid-this": 0, // to be fixed
        "no-iterator": 2,
        "no-lone-blocks": 2,
        "no-loop-func": 2,
        "no-magic-numbers": 0, // to be fixed
        "no-multi-str": 2,
        "no-native-reassign": 2,
        "no-param-reassign": 0, // to be fixed
        "no-new-func": 2,
        "no-new-wrappers": 2,
        "no-new": 2,
        "no-octal-escape": 2,
        "no-octal": 2,
        "no-proto": 2,
        "no-redeclare": 0,
        "no-return-assign": 2,
        "no-script-url": 2,
        "no-self-compare": 2,
        "no-sequences": 2,
        "no-throw-literal": 2,
        "no-unused-expressions": 2,
        "no-useless-call": 2,
        "no-useless-concat": 0, // to be fixed
        "no-with": 2,
        "radix": 2,
        "wrap-iife": 2,
        "yoda": [
            2,
            "never"
        ],
        /* Stylistic Issues */
        "array-bracket-spacing": [
            0,
            "never"
        ],
        "block-spacing": [
            0,
            "always"
        ],
        "brace-style": [
            0,
            "1tbs"
        ],
        "camelcase": 0,
        "comma-spacing": [
            2,
            {
                "before": false,
                "after": true
            }
        ],
        "comma-style": [
            2,
            "last"
        ],
        "computed-property-spacing": [
            2,
            "never"
        ],
        "consistent-this": [
            0,
            "that"
        ], // to be fixed
        "eol-last": 0,
        "func-names": 0,
        "func-style": [
            0,
            "declaration"
        ],
        "id-length": [
            1,
            {
                "min": 2,
                "properties": "never",
                "exceptions": [
                    "i",
                    "j",
                    "$",
                    "_"
                ]
            }
        ],
        "id-match": 0,
        "indent": 0,
        "jsx-quotes": 0,
        "key-spacing": [
            0,
            {
                "beforeColon": true,
                "afterColon": true
            }
        ],
        "linebreak-style": [
            0,
            "windows"
        ],
        "lines-around-comment": 0,
        "max-nested-callbacks": [
            0,
            2
        ],
        "new-cap": [
            2,
            {
                "newIsCap": true,
                "capIsNew": false,
                "capIsNewExceptions": [
                    "Event"
                ]
            }
        ],
        "new-parens": 2,
        "newline-after-var": 0,
        "no-array-constructor": 2,
        "no-continue": 0,
        "no-inline-comments": 0,
        "no-lonely-if": 0,
        "no-mixed-spaces-and-tabs": [
            2,
            false
        ],
        "no-multiple-empty-lines": [
            0,
            {
                "max": 2
            }
        ],
        "no-negated-condition": 0,
        "no-nested-ternary": 0,
        "no-new-object": 2,
        "no-restricted-syntax": 0,
        "no-spaced-func": 2,
        "no-ternary": 0,
        "no-trailing-spaces": 0,
        "no-underscore-dangle": 0,
        "no-unneeded-ternary": 2,
        "object-curly-spacing": [
            0,
            "always"
        ],
        "one-var": [
            2,
            "never"
        ],
        "operator-assignment": [
            0,
            "always"
        ],
        "operator-linebreak": [
            2,
            "after"
        ],
        "padded-blocks": 0,
        "quote-props": [
            2,
            "as-needed",
            {
                "keywords": true,
                "unnecessary": false
            }
        ],
        "quotes": [
            1,
            "double",
            "avoid-escape"
        ],
        "require-jsdoc": 0,
        "semi-spacing": [
            1,
            {
                "before": false,
                "after": true
            }
        ],
        "semi": 2,
        "sort-vars": 0,
        "space-before-blocks": [
            1,
            "always"
        ],
        "space-before-function-paren": [
            1,
            "never"
        ],
        "keyword-spacing": 1,
        "space-in-parens": [
            0,
            "never"
        ],
        "space-infix-ops": [
            1,
            {
                "int32Hint": true
            }
        ],
        "space-unary-ops": [
            2,
            {
                "words": true,
                "nonwords": false
            }
        ],
        "spaced-comment": 0,
        "wrap-regex": 2,
        /** Expressly Disabled **/
        /** Legacy **/
        "max-len": [
            0,
            80,
            4
        ],
        "max-params": [
            0,
            3
        ],
        "max-statements": [
            0,
            10
        ],
        "no-plusplus": 0,
        /** ECMAScript 6 **/
        "arrow-body-style": 0,
        "arrow-parens": 0,
        "arrow-spacing": 0,
        "constructor-super": 0,
        "generator-star-spacing": 0,
        "no-arrow-condition": 0,
        "no-class-assign": 0,
        "no-const-assign": 0,
        "no-dupe-class-members": 0,
        "no-this-before-super": 0,
        "no-var": 0,
        "object-shorthand": 0,
        "prefer-arrow-callback": 0,
        "prefer-const": 0,
        "prefer-reflect": 0,
        "prefer-spread": 0,
        "prefer-template": 0,
        "require-yield": 0,
        /** Variables **/
        "init-declarations": 0,
        "no-use-before-define": 0,
        /** Best practices **/
        "block-scoped-var": 0,
        "no-multi-spaces": 0,
        "no-process-env": 0,
        "no-void": 0,
        "no-warning-comments": [
            0,
            {
                "terms": [
                    "todo",
                    "fixme",
                    "xxx"
                ],
                "location": "start"
            }
        ],
        "vars-on-top": 0
    }
}